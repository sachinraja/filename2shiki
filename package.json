{
  "name": "filename2shiki",
  "version": "1.1.0",
  "type": "module",
  "description": "filename to Shiki language",
  "repository": {
    "type": "git",
    "url": "https://github.com/sachinraja/filename2shiki.git"
  },
  "homepage": "https://github.com/sachinraja/filename2shiki#readme",
  "bugs": {
    "url": "https://github.com/sachinraja/filename2shiki/issues"
  },
  "author": "Sachin Raja <sachinraja2349@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "shiki"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build-grammar && tsup src/index.ts --format esm --dts",
    "build-grammar": "node --loader ts-node/esm scripts/build.ts",
    "clean": "rimraf dist",
    "format": "prettier -w .",
    "lint": "run-p lint:*",
    "lint:format": "prettier -c .",
    "lint:types": "tsc",
    "lint:xo": "xo",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@swc/core": "1.2.111",
    "@swc/jest": "0.2.5",
    "@types/jest": "27.0.3",
    "@types/js-yaml": "4.0.5",
    "@types/node": "16.11.9",
    "eslint-config-prettier": "8.3.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "js-yaml": "4.1.0",
    "lint-staged": "11.2.6",
    "npm-run-all": "4.1.5",
    "param-case": "3.0.4",
    "prettier": "2.4.1",
    "prettier-plugin-pkg": "0.11.0",
    "regenerator-runtime": "0.13.9",
    "rimraf": "3.0.2",
    "shiki": "0.9.12",
    "ts-node": "10.4.0",
    "tsup": "5.7.2",
    "typescript": "4.4.4",
    "xo": "0.46.4"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,ts,md,json,yaml}": "prettier -w"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "./node_modules/prettier-plugin-pkg"
    ]
  },
  "xo": {
    "extends": [
      "prettier"
    ],
    "ignores": [
      "src/language.ts"
    ]
  }
}
